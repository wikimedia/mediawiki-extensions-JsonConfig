		"EditPageCopyrightWarning": "JsonConfig\\JCHooks::onEditPageCopyrightWarning",
		"MovePageIsValidMove": "JsonConfig\\JCHooks::onMovePageIsValidMove",
		"PageSaveComplete": "JsonConfig\\JCHooks::onPageSaveComplete",
		"ScribuntoExternalLibraries": "JsonConfig\\JCLuaLibrary::onScribuntoExternalLibraries",
		"SkinCopyrightFooter": "JsonConfig\\JCHooks::onSkinCopyrightFooter",
		"TitleGetEditNotices": "JsonConfig\\JCHooks::onTitleGetEditNotices",
		"PageMoveComplete": "JsonConfig\\JCHooks::onPageMoveComplete",
		"getUserPermissionsErrors": "JsonConfig\\JCHooks::onGetUserPermissionsErrors"
