{
	"name": "JsonConfig",
	"version": "1.2.0",
	"author": [
		"Yuri Astrakhan",
		"Eranroz"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:JsonConfig",
	"descriptionmsg": "jsonconfig-desc",
	"type": "other",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.41"
