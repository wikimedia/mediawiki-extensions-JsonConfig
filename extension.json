		"JsonConfigModels": {
			"merge_strategy": "array_replace_recursive",
			"value": {}
		},
		"JsonConfigDisableCache": {
			"value": false
		},
		"JsonConfigCacheKeyPrefix": {
			"value": "1"
		},
		"JsonConfigEnableLuaSupport": {
			"value": false
		},
		"JsonConfigAllowedLicenses": {
			"value": [ "CC0-1.0" ]
		},
		"JsonConfigInterwikiPrefix": {
			"value": "commons"
		}
	},
	"manifest_version": 2
