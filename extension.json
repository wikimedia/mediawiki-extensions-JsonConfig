				"CC-BY-SA-1.0",
				"CC-BY-SA-2.0",
				"CC-BY-SA-2.5",
				"CC-BY-SA-3.0",
				"CC-BY-SA-4.0",
				"CC-BY-SA-4.0+",
				"ODbL-1.0",
				"dl-de-zero-2.0",
				"dl-de-by-1.0",
				"dl-de-by-2.0",
				"GeoNutzV"
			]
		},
		"JsonConfigUseGUI": {
			"value": true
		}
	},
	"manifest_version": 2
}
