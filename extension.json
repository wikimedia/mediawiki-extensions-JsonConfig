	"namespaces": [
		{
			"id": 482,
			"constant": "NS_CONFIG",
			"name": "Config",
			"conditional": true
		},
		{
			"id": 483,
			"constant": "NS_CONFIG_TALK",
			"name": "Config_talk",
			"conditional": true
		},
		{
			"id": 486,
			"constant": "NS_DATA",
			"name": "Data",
			"conditional": true
		},
		{
			"id": 487,
			"constant": "NS_DATA_TALK",
			"name": "Data_talk",
			"conditional": true
		}
	],
	"MessagesDirs": {
		"JsonConfig": [
			"i18n",
			"i18n/api"
		]
	},
	"ExtensionMessagesFiles": {
		"JsonConfigNamespaces": "JsonConfig.namespaces.php"
	},
	"RawHtmlMessages": [
		"jsonconfig-license"
	],
